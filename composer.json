{
    "name": "flyntwp/premium-plugin-downloader",
    "description": "The Premium Plugin downloader helps you to automatically download premium plugins such as paid WordPress plugins from a remote, possibly password protected server.",
    "type": "library",
    "license": "MIT",
    "bin": ["bin/premium-plugin-downloader"],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "nabil1337/case-helper": "^0.1.0",
        "symfony/console": "^4.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.9.1 || ^3.3.1"
    },
    "autoload": {
        "psr-4": {"FlyntWP\\PremiumPluginDownloader\\": "src/"}
    }
}
